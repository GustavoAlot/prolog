antecessor(X,Y) :- pai(X,Y).
antecessor(X,Y) :- mae(X,Y).
antecessor(X,Y) :- mae(X,Z), antecessor(Z,Y).
antecessor(X,Y) :- pai(X,Z), antecessor(Z,Y).


