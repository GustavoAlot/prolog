irmao(X,Y) :- pai(Z,X),pai(Z,Y), X \= Y.

       
primo(X,Y) :- pai(Z,X), pai(W,Y), irmao(Z,W).
primo(X,Y) :- mae(Z,X), mae(W,Y), irmao(Z,W).
